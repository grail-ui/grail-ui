{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm api && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --cache .",
    "api": "svelte-kit sync && pnpm --filter=\"@grail-ui/svelte\" api --json ../../apps/docs/src/docs/api.json"
  },
  "devDependencies": {
    "@iconify/json": "2.1.146",
    "@sveltejs/adapter-auto": "1.0.0-next.90",
    "@sveltejs/kit": "1.0.0-next.567",
    "@tailwindcss/typography": "0.5.8",
    "@types/prismjs": "1.26.0",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "autoprefixer": "10.4.13",
    "change-case": "4.1.2",
    "daisyui": "2.42.1",
    "eslint": "8.28.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.19",
    "prism-svelte": "0.5.0",
    "prismjs": "1.29.0",
    "svelte": "3.53.1",
    "svelte-check": "2.10.0",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.2.4",
    "tslib": "2.4.1",
    "typedoc": "0.23.21",
    "typedoc-json-parser": "7.0.2",
    "typescript": "4.9.3",
    "unplugin-icons": "0.14.14",
    "vite": "3.2.4"
  }
